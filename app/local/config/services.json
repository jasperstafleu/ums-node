{
  "controller.main": {
    "class": "$stafleu_local/Controller/Main",
    "arguments": [
      "@session.bag",
      "@repository.user"
    ],
    "tags": [
      {
        "name": "controller",
        "action": "index",
        "route": "^/$",
        "defaults": {
          "string0": "world",
          "request1": ""
        }
      },
      {
        "name": "controller",
        "action": "index",
        "route": "^/test/(?<string0>[^/]+)$",
        "defaults": {
          "request1": ""
        }
      }
    ]
  },
  "repository.user": {
    "class": "$stafleu_local/Database/UserRepository",
    "arguments": [
      "@query_executor"
    ]
  }
}
